{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage",
  "coverageReporters" : ["json", "lcov", "text", "clover"],
  "coveragePathIgnorePatterns": [
    ".module.ts$",
    ".spec.ts$"
  ]
}
